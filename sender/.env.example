# Telegram Parser Account (for reading leads)
TG_PARSER_API_ID=
TG_PARSER_API_HASH=
TG_PARSER_SESSION=

# Telegram Sender Account (for sending messages)
TG_SENDER_API_ID=
TG_SENDER_API_HASH=
TG_SENDER_SESSION=

# OpenAI API key (direct OpenAI)
OPENAI_API_KEY=

# Lead inbox (where parser sends leads)
LEAD_INBOX_PEER_ID=
LEAD_INBOX_TOPIC_ID=1

# Report thread (for logging)
REPORT_PEER_ID=
REPORT_TOPIC_ID=

# Timezone settings
TIMEZONE=Europe/Berlin
SENDER_TIMEZONE=Europe/Moscow

# Work window (when to send messages, 24h format)
WORK_WINDOW_START=00:00
WORK_WINDOW_END=23:59

# Rate limiting
RATE_LIMIT_MAX_MESSAGES=3
RATE_LIMIT_INTERVAL_MIN=90000
RATE_LIMIT_INTERVAL_MAX=180000
RATE_LIMIT_BURST_PAUSE=1800000

# OpenAI settings
OPENAI_MODEL=gpt-4o-mini
OPENAI_MAX_TOKENS=500
OPENAI_TEMPERATURE=0.7

# Logging (trace, debug, info, warn, error)
LOG_LEVEL=info