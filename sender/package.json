{
  "name": "telegram-sender",
  "version": "2.0.0",
  "description": "Telegram outreach sender module for lead processing",
  "type": "module",
  "scripts": {
    "start": "chcp 65001 >nul 2>&1 & node src/app.js",
    "start:unix": "node src/app.js",
    "dev": "chcp 65001 >nul 2>&1 & node src/app.js",
    "login:sender": "chcp 65001 >nul 2>&1 & node src/sender.login.js",
    "lint": "eslint src --ext .js",
    "lint:fix": "eslint src --ext .js --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\""
  },
  "dependencies": {
    "dayjs": "^1.11.19",
    "dotenv": "^16.4.5",
    "openai": "^6.0.0",
    "pino": "^8.17.2",
    "pino-pretty": "^10.3.1",
    "telegram": "^2.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
