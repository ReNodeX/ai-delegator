# Telegram Parser (основной аккаунт для парсинга)
TG_API_ID=
TG_API_HASH=
TG_SESSION=

# Telegram Parser для Sender (аккаунт для чтения лидов)
TG_PARSER_API_ID=
TG_PARSER_API_HASH=
TG_PARSER_SESSION=

# Telegram Sender (аккаунт для отправки сообщений)
TG_SENDER_API_ID=
TG_SENDER_API_HASH=
TG_SENDER_SESSION=

# OpenAI
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini
OPENAI_MAX_TOKENS=500
OPENAI_TEMPERATURE=0.7

# Целевая группа для пересылки лидов (парсер)
TARGET_GROUP_ID=

# Inbox для лидов (sender)
LEAD_INBOX_PEER_ID=
LEAD_INBOX_TOPIC_ID=1

# Чат/топик для уведомлений об отправке (sender)
# Если не указан REPORT_PEER_ID - используется LEAD_INBOX_PEER_ID
# Если не указан REPORT_TOPIC_ID - используется LEAD_INBOX_TOPIC_ID
REPORT_PEER_ID=
REPORT_TOPIC_ID=

# Часовые пояса
TIMEZONE=Europe/Berlin
SENDER_TIMEZONE=Europe/Moscow

# Рабочее окно отправки (sender)
WORK_WINDOW_START=09:00
WORK_WINDOW_END=21:00

# Rate limiting (sender)
RATE_LIMIT_MAX_MESSAGES=3
RATE_LIMIT_INTERVAL_MIN=90000
RATE_LIMIT_INTERVAL_MAX=180000
RATE_LIMIT_BURST_PAUSE=1800000

# AI классификатор (парсер)
AI_MODEL=gpt-4o-mini
AI_MIN_CONFIDENCE=0.7

# Backfill
BACKFILL_DAYS=1

# Логирование
LOG_LEVEL=info

